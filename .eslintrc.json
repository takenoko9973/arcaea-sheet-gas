{
  "env": {
    "browser": true,
    "node": true,
    "googleappsscript/googleappsscript": true,
    "es2020": true
  },
  "extends": [
    "./node_modules/gts",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": [
    "prettier",
    "@typescript-eslint",
    "googleappsscript"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": [
    "dist/**/*.js"
  ]
}
